shared.VapeIndependent = true
shared.CustomSaveVape = "Night"
local uilib = loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
local Render = uilib["ObjectsThatCanBeSaved"]["RenderWindow"]["Api"]
local esp = Render.CreateOptionsButton({
    Name = "Night",
    HoverText = "Your Bedtime Little Boy",
    Function = function(v)
        game.Lighting:SetMinutesAfterMidnight(0)
     end
})

local Blatant = uilib["ObjectsThatCanBeSaved"]["BlatantWindow"]["Api"]
local esp = Blatant.CreateOptionsButton({
    Name = "HypixelFly",
    HoverText = "Basically Hypixel Fly",
    Function = function(v)
        longjumpval = v
        if longjumpval then
            workspace.Gravity = 0
            spawn(function()
                repeat
                    if (not longjumpval) then return end
                    lplr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
                    wait(0.1)
                    lplr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
                    wait(0.1)
                    lplr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Climbing)
                    wait(0.1)
                    lplr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
                    wait(0.1)
                    lplr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Landed)
                    wait(0.1)
                until (not longjumpval)
            end)
        else
            workspace.Gravity = 196.19999694824
            return
        end
    end
})

local Blatant = uilib["ObjectsThatCanBeSaved"]["BlatantWindow"]["Api"]
local esp = Blatant.CreateOptionsButton({
    Name = "LowGravity",
    Function = function(v)
        if v == true then
            workspace.Gravity = 10
        else
            workspace.Gravity = 196.19999694824
        end
    end

})

local World = uilib["ObjectsThatCanBeSaved"]["WorldWindow"]["Api"]
local esp = World.CreateOptionsButton({
    Name = "AntiVoid",
    Function = function(v)
        local antivoidpart
        if v then
            local antivoidpart = Instance.new("Part", Workspace)
            antivoidpart.Name = "AntiVoid"
            antivoidpart.Size = Vector3.new(2100, 0.5, 2000)
            antivoidpart.Position = Vector3.new(160.5, 25, 247.5)
            antivoidpart.Transparency = 0.4
            antivoidpart.Anchored = true
            antivoidpart.Touched:connect(function(dumbcocks)
                if dumbcocks.Parent:WaitForChild("Humanoid") and dumbcocks.Parent.Name == lplr.Name then
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
                    wait(0.2)
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
                    wait(0.2)
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
                end
            end)
        else
            game.Workspace.AntiVoid:remove()
        end
    end
})

local Blatant = uilib["ObjectsThatCanBeSaved"]["BlatantWindow"]["Api"]
local esp = Blatant.CreateOptionsButton({
    Name = "InfJump",
    Function = function()
        game:GetService("UserInputService").JumpRequest:connect(function()
            game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")       
        end)
    end
})

local Utility = uilib["ObjectsThatCanBeSaved"]["UtilityWindow"]["Api"]
local esp = Utility.CreateOptionsButton({
    Name = "AntiCheatDisabler",
    HoverText = "Disables Anticheat (Requires Minicopter)",
    Function = function()
        for i, v in pairs(game.Workspace:GetChildren()) do
            if v.Name == "minicopter" then
                    if (not v:FindFirstChild("Seat")) then
                            v:remove()
                    else
                            game:GetService("Workspace").minicopter.Seat:remove()
                            wait(1)
                            game:GetService("Workspace").minicopter.Driver:remove()
                    end
            end
        end   
    end
})

local Blatant = uilib["ObjectsThatCanBeSaved"]["BlatantWindow"]["Api"]
local esp = Blatant.CreateOptionsButton({
    Name = "Speed",
    HoverText = "Only Works With Disabler",
    Function = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 53
    end
})




  

